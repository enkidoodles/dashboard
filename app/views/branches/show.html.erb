<div class="container", id="branchnewform">
	<p id="notice"><%= notice %></p>

	<div class="form-group">
	  <label>Name:</label>
	  <input type="text" class="form-control" value="<%= @branch.name %>" disabled>
	</div>

	<div class="form-group">
		<label>Branch json url:</label>
		<textarea class="form-control" readonly>
			<%= @branch.branch_json_url %>
		</textarea>
		
	</div>

	<div class="row">
		<div class="col-sm-6" id="editbutton">
			<%= link_to 'Edit', edit_branch_path(@branch) , :class=>"btn btn-secondary"%>
		</div>
		<div class="col-sm-6" id="backbutton">
			<%= link_to 'Back', branches_path , :class=>"btn btn-secondary"%>
		</div>
	</div>
</div>
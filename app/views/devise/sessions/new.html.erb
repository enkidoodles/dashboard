<div class="container-fluid">
  <div class="row" id="header">
    <div class="col-lg-6" id="logodiv">
      <img id="logo" src="/nokiaLogo.png" alt="rss feed" />
    </div>
    <div class="col-lg-6" id="links">
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>

<div class="container-fluid" id="body">
  <div class="jumbotron" id="login">
    <div class="inner">
      <center><h2>Log in</h2></center>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, autofocus: true %>
        </div>

        <div class="form-group">
          <%= f.label :password %>
          <%= f.password_field :password, autocomplete: "off" %>
        </div>

        <center><div class="form-check" id="loginbutton">
           <%= f.submit "Log in" , :class => "btn btn-secondary"%>
        </div></center>
         
      <% end %>
    </div>
  </div>
</div>